    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0 || ^16.0.0-0",
    "realm": "^0.15.0 || ^1.0.0"
  },
  "dependencies": {
    "hoist-non-react-statics": "^1.2.0",
    "invariant": "^2.0.0",
    "lodash.isplainobject": "^4.0.6",
    "prop-types": "^15.5.10"
  },
